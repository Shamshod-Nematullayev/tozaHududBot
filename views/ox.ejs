    <main style="display: flex">
      <table style="padding-bottom: 215px">
        <tr>
          <td>
            <div class="col">
              <div>
                <h3 style="text-align: center">
                  ОГОХЛАНТИРИШ ХАТИ <span fp="fp_nomer"></span>
                </h3>
              </div>
              <table
                style="
                  width: 100%;
                  margin-bottom: 12px;
                  display: flex;
                  justify-content: space-around;
                  margin-top: 2px;
                "
